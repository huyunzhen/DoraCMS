<div class="table-responsive">
<%if(newCourseData.length!=0 ){%>
  <table class="table table-bordered table-condensed">
    <tr>
        <th>日期</th>
        <%newCourseData[0].courses.forEach(function(item){%>
        <th><%=item.contentTagsTitle%><br/>当日数量</th>
        <th><%=item.contentTagsTitle%><br/>累计数量</th>
        <%})%>
        <th>回向文</th>
    </tr>
    
    
    <%userCourseListData.forEach(function(items){%>
    <tr>
        <td class=""><%=myDateFormat(items.date)%></td>
        <%items.courses.forEach(function(item){%>
        <td><%=item.todayNum%></td>
        <td><%=item.totalNum%></td>
        <%})%>
        <td class=" "><%=items.contents%></td>
    </tr>
    <%})%>
    
  </table>
 <%}%>
</div>
